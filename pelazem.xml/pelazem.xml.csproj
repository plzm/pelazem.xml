    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Authors>Patrick El-Azem (www.linkedin.com/in/pelazem)</Authors>
    <Description>A utility library with functionality to parse XML.</Description>
    <Copyright>Patrick El-Azem (www.linkedin.com/in/pelazem)</Copyright>
    <PackageProjectUrl>https://github.com/plzm/pelazem.xml</PackageProjectUrl>
    <RepositoryUrl>https://github.com/plzm/pelazem.xml</RepositoryUrl>
    <AssemblyVersion>7.0.0.0</AssemblyVersion>
    <FileVersion>7.0.0.0</FileVersion>
    <Version>7.0.0</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="pelazem.util" Version="3.0.2" />
  </ItemGroup>

</Project>
